		<Space Name="LeftFrame" Size="1"/>
		
		<HBoxLayout Name="WBMoveCategoryContent" SizePolicy="Spring;Spring">
			<ScrollArea Name="WBCategoryList" SizePolicy="Spring;Spring" Background="#FFFFFF">
