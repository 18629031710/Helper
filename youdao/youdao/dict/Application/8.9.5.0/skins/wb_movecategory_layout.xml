					<Spring/>
					<TextButton Text=""/>
					<PopAttr SizePolicy=""/>
				</VBoxLayout>
			</ScrollArea>
			<ScrollBar Name="WBCategoryScrollBar" SizePolicy="Fix;Spring" Width="8" Type="Vertical" Background="#FFFFFF" BarColor="#D6D6D6;#C1C1C1" Delta="5"/>
		</HBoxLayout>
		
		<Space Name="RightFrame" Size="1"/>
		</HBoxLayout>
		<Space Name="BottomFrame" Size="1"/>
		</VBoxLayout>
	</Layout>
</WBMoveCategory>